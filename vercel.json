{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main",
        "methods" : [
            "GET",
            "PUT",
            "POST",
            "DELETE"
        ]
      }
    ]
  }